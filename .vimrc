" :nnoremap <Leader>zz :let &scrolloff=999-&scrolloff<CR>
set so=999
set nocompatible
filetype indent plugin on
syntax on
set wildmenu
set showcmd
set ignorecase
set smartcase
set autoindent
set backspace=indent,eol,start
set nostartofline
set ruler
set laststatus=2
"set cmdheight=2
set confirm
set mouse=a
set shiftwidth=4
set softtabstop=4
set expandtab
set rtp+=/usr/lib/python3.6/site-packages/powerline/bindings/vim
